<?xml version="1.0" encoding="utf-8" ?>
<rxui:ReactiveContentPage 
    x:Class="Scoopy.Views.MainPage"
    x:TypeArguments="vm:MainPageVM"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
    xmlns:vm="clr-namespace:Scoopy.ViewModels;assembly=Scoopy"
    xmlns:v="clr-namespace:Scoopy.Views;assembly=Scoopy"
    xmlns:local="clr-namespace:Scoopy"
    >

    <StackLayout x:Name="pnlMain" VerticalOptions="CenterAndExpand" HorizontalOptions="Center" WidthRequest="300">
        <v:ConnectView x:Name="ConnectView" Margin="20" />
    </StackLayout>

</rxui:ReactiveContentPage>
